swers-color: hsl(240, 6%, 50%);\n$border-color: hsl(240, 5%, 91%);\n","scss/_base.scss":"* {\n    box-sizing: border-box;\n    margin: 0;\n    padding: 0;\n}\n\nbody {\n    font-size: 12px;\n    font-family: $family;\n    height: 100%;\n    background-image: $bg-gradient;\n    background-repeat: no-repeat;\n}\n\nh1 {\n    font-size: $big-heading-size;\n    font-weight: $bold;\n}\n\nh2 {\n    font-size: $questions-size;\n    margin: 0;\n}\n\np {\n    font-size: $answers-size;\n}\n\nmain {\n    height: 100vh;\n    margin: 0 auto;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.container {\n    align-items: center;\n    justify-content: center;\n    background-color: white;\n    border-radius: 20px;\n    padding: 0;\n    margin: 0 100px;\n    background-image: url(\"../images/bg-pattern-desktop.svg\");\n    background-repeat: no-repeat;\n    background-size: 80rem; \n    background-position: -50rem -25rem;\n}\n\n@media screen and (max-width: 900px) {\n    main {\n        margin: 30% auto 5%;\n    }\n\n    .container {\n        background-image: url(\"../images/bg-pattern-mobile.svg\");\n        background-size: 70%; \n        background-position: top center;\n        transform: scale(0.9);\n    }\n}","scss/_image-side.scss":".image-side {\n    position: relative;\n}\n\n.desktop-img {\n    overflow: hidden;\n    position: relative;\n}\n\n.desktop-illust {\n    display: flex;\n    align-items: center;\n    position: relative;\n    left: -7rem;\n    width: 38rem;\n}\n\n.box-illust {\n    width: 14rem;\n    position: absolute;\n    top: 11.5rem;\n    left: -7rem;\n}\n\n.mobile-img {\n    display: none;\n}\n\n@media screen and (max-width: 900px) {\n\n    .desktop-img, .desktop-illust, .box-illust {\n        display: none;\n        position: static;\n    }\n\n    .image-side {\n        width: 100%;\n    }\n\n    .mobile-img {\n        display: block;\n        width: 70%;\n        margin: 0 auto;\n        position: absolute;\n        left: 50%;\n        top: 50%;\n        transform: translate(-50%, -60%);\n    }\n\n}","scss/_faqs-side.scss":".faq-side {\n    width: 100%;\n    padding: 100px 100px 100px 0;\n}\n\n.faq-heading {\n    margin-bottom: 30px;\n}\n\n.questions {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    width: 100%;\n    font-size: $questions-size;\n    color: $bold-text-color;\n    border: 0;\n    background-color: transparent;\n    text-align: left;\n    margin-bottom: 20px;\n    cursor: pointer;\n    :hover {\n        color: $orange-hover-text;\n    }\n}\n\n.arrow-down {\n    width: 15px;\n    transition: all 0.2s ease-in;\n}\n\n.faq-1, .faq-2, .faq-3, .faq-4, .faq-5 {\n    border-bottom: 1px solid $border-color;\n    margin-top: 20px;\n}\n\n// span {\n//     display: none;\n// }\n\n.answers {\n    display: none;\n    color: $answers-color;\n    width: 95%;\n    margin-bottom: 30px;\n}\n\n// To add by JavaScript\n\n.active { \n    span {\n        color: $bold-text-color;\n        font-weight: $bold;\n    }\n    img {\n        transform: rotate(180deg);\n        transition: all 0.2s ease-in;\n    }\n    p {\n        font-weight: $regular;\n        display: block;\n    }\n}\n\n.move-box {\n    left: -10rem;\n}\n\n@media screen and (max-width: 900px) {\n    .faq-side {\n        padding: 25px 25px 50px;\n        margin-top: 120px;\n    }\n\n    .faq-heading {\n        text-align: center;\n    }\n}"},"lineCount":null}},"error":null,"hash":"0019857ed719f58d8e363fef18e8548a","cacheData":{"env":{}}}